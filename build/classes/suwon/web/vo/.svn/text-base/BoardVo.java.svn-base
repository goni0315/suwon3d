package suwon.web.vo;

public class BoardVo {
	private int adf_seq;					//첨부파일 정보(ADF_INFO)테이블의 첨부파일 일련번호
	private String bod_ctt; 				//공지사항(TN_BOARD)테이블의 내용
	private String mng_nm;				//관리자(MNG_NAM)테이블의 이름
	
	public int getAdf_seq() {
		return adf_seq;
	}
	public void setAdf_seq(int adf_seq) {
		this.adf_seq = adf_seq;
	}
	public String getBod_ctt() {
		return bod_ctt;
	}
	public void setBod_ctt(String bod_ctt) {
		this.bod_ctt = bod_ctt;
	}
	public String getMng_nm() {
		return mng_nm;
	}
	public void setMng_nm(String mng_nm) {
		this.mng_nm = mng_nm;
	}
}
