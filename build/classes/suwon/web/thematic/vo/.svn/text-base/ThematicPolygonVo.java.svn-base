package suwon.web.thematic.vo;

public class ThematicPolygonVo {	
	
	private String gid;
	private String pnu;
	private String jibun;
	private String bchk;
	private String geom;
	
	public ThematicPolygonVo(String pnu){
		setPnu(pnu);
	}
	
	public String getGid() {
		return gid;
	}
	public void setGid(String gid) {
		this.gid = gid;
	}
	public String getPnu() {
		return pnu;
	}
	public void setPnu(String pnu) {
		this.pnu = pnu;
	}
	public String getJibun() {
		return jibun;
	}
	public void setJibun(String jibun) {
		this.jibun = jibun;
	}
	public String getBchk() {
		return bchk;
	}
	public void setBchk(String bchk) {
		this.bchk = bchk;
	}
	public String getGeom() {
		return geom;
	}
	public void setGeom(String geom) {
		this.geom = geom;
	}
}