<script type="text/javascript">
function goPlay(popUp_path){
	var w=window.open(WEB_SERVER_URL+"/help/pop/help_pop.jsp?popUp_path="+popUp_path,"_blank","width=1324px,height=750px");
	var winxpos = (window.screen.width-1324)/2;
	var winypos = (window.screen.height-750)/2;
	w.moveTo(winxpos,winypos);		
}
function doVOD(val){
	location.replace(WEB_SERVER_URL_SB + "/helpVideo/" + val + ".flv");
}	
</script>
