@charset "euc-kr";

/* Type Selector */
*						{margin:0; padding:0;}
ul,li					{list-style:none;}
img, fieldset, button	{border:none;}
hr, button img			{display:none;}
a						{text-decoration:none;}
a:hover, a:active, a:focus{text-decoration:underline;}
textarea				{overflow:auto;}
legend					{position:absolute; top:0; left:0; width:0; height:0; overflow:hidden; visibility:hidden; font-size:0; line-height:0;} /* For Screen Reader */
form					{margin:0; border: 0;}
select		{background-color:#e5e5e5; color:#6D4400; font-family: Dotum,  Gulim, AppleGothic, Sans-serif;	font-size:11px; line-height:140%;}
p,br,body,table,td,select,input,form,textarea,option {
	font-family: Dotum, Gulim, AppleGothic, Sans-serif;
	color: #555555;
	font-size:11px;
	line-height: 1em;
}

.clear		{clear:both;}
.textarea	{font-size:12px; line-height:120%; font-family: Dotum, Gulim, AppleGothic, Sans-serif; border: 1px solid #dddddd;background-color: #ffffff;}
.input		{height:17px; font-size:12px; font-family: Dotum,Gulim,seoul,helvetica,verdana; border:1px solid #cccccc; background-color: #ffffff;}
.input_yellow {height:17px; font-size:12px; line-height:120%; font-family: Dotum,Gulim,seoul,helvetica,verdana; border: 1px solid #ddd8b5; background-color: #f4f2e6;}
.input_gray {font-size:12px; line-height:120%; font-family: Dotum,Gulim,seoul,helvetica,verdana; background-color: #e5e5e5; border-style:none; border-width:0px;}

