@charset "utf-8";
body, p, h1, h2, h3, h4, h5, h6, menu, ul, ol, li, dl, dt, dd, table, th, td, form, input, textarea, button {
	margin : 0;
	padding : 0;
}
fieldset { border: 1px  outset #9f9f9f ; }
textfield {
	height:23px; border:solid 1px }
html, body {
	height : 100%;
	overflow:hidden;
}
body, input, textarea, select, button, table, pre {
	font-family : '돋움', Dotum, Helvetica, 'Apple SD Gothic Neo', Sans-serif;
	font-size : 12px;
}
img, button	{border:none;}
hr, button img			{display:none;}

menu, ul, ol {
	list-style : none;
}
em, address {
	font-style : normal;
}
a, a:link {
	text-decoration : none;border : 0;
}
a:hover, a:active, a:focus {
	text-decoration : underline;border : 0;
}
textarea {
	overflow : auto;
}
option, x:-moz-any-link {
 padding-right : 6px;
}
input[type="checkbox"], input[type="radio"] {
	width : 13px;
	height : 13px;
}
.blind {
	visibility : hidden;
	overflow : hidden;
	position : absolute;
	top : 0;
	left : 0;
	width : 0;
	height : 0;
	font-size : 0;
	line-height : 0;
}
button, label {
	cursor : pointer;
}
label { float:left; width:6em; color:#61697e; font-weight:bold; height:22px; margin-top:3px;}
hr {
	display : none;
}
caption {
	overflow : hidden;
	position : relative;
	top : -100px;
	left : -100px;
	width : 0;
	height : 0;
	font-size : 0;
	line-height : 0;
}
#u_skip {
	position : relative;
	z-index : 2147483647;
}
#u_skip a {
	position : absolute;
	left : 0;
	top : -30px;
	width : 108px;
	background : url(http://static.naver.net/maps/ux2013/sp_map_vertical_v4.png) repeat-x 0 -91px;
	border : #4ec53d solid 1px;
	text-align : center;
	outline : none;
}
#u_skip a:active, #u_skip a:focus {
	top : 0;
	text-decoration : none;
	cursor : pointer;
}
#u_skip span {
	padding : 2px 6px 0 0;
	background : url(http://static.naver.net/maps/ux2013/sp_map_vertical_v4.png) no-repeat 100% -69px;
	font-size : 11px;
	letter-spacing : -1px;
	color : #fff;
	line-height : 26px;
}
.aside_footer {
	text-align : left;
	float:left;
}
.afooter_lst {
	float:left;
}
.aside_footer .ul {
	float:left;
}

.copyright {
	position : static;
	height : 0;
	font-family : '나눔고딕', NanumGothic, '돋움', Dotum, Helvetica, 'Apple SD Gothic Neo', Sans-serif;
	font-size : 11px;
	white-space : nowrap;
	line-height : 17px;
}
.copyright a.limit {
	margin-left : 10px;
	padding-right : 6px;
	background : url(http://static.naver.net/maps/ux2013/sp_map_vertical_v4.png) no-repeat 100% -216px;
	color : #000;
	font-weight : bold;
}
.copyright p {
	position : absolute;
	bottom : 4px;
	left : 10px;
}
.wrap_satellite .copyright {
	color : #fff;
}
.wrap_satellite .copyright a.limit {
	color : #fff;
}
#wrap {
	position : relative;
	height : 100%;
}
#header {
	position : absolute;
	top : 0;
	left : 0;
	z-index : 30;
	width : 100%;
	min-width : 980px;
	height : 99px;
	background : url(../images/top_bg.gif) repeat-x;
}
#container {
	position : absolute;
	top : 99px;
	right : 0;
	bottom : 0;
	left : 0;
	z-index : 20;
}
#aside {
	position :absolute;
	top : 0;
	bottom : 0;
	left : 0;
	z-index : 3;
	width : 339px;
	height:99.9%;
	border-right : 1px solid #282e3e;
	background-color : #fff;
}
#aside .aside_line {
	position : absolute;
	top : 0;
	bottom : 0;
	right : -2px;
	z-index : 2;
	width : 1px;
	height : 100%;
	background : #000;
}
#aside_footer {
	position : absolute;
	right : 0;
	bottom : 0;
	left : 0;
	z-index : 30;
	height : 26px;
	float: left;
	border-top : 1px solid #bcbcbc;
	background : #fafafa;
}
#content {
	position : absolute;
	top : 0;
	right : 0;
	bottom : 0;
	left : 0px;
	width : 100%;
	background : #f1f1f1;
}
.snb_line {
	position : absolute;
	right : 0;
	bottom : -2px;
	left : 0;
	width : 100%;
	height : 1px;
	background : #000;
}
