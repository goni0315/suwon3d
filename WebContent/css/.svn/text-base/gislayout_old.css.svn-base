@charset "utf-8";
/* NTS UI Development 1Team HJS 130403 */

/* Common */
body,p,h1,h2,h3,h4,h5,h6,menu,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button{margin:0;padding:0}
html,body{overflow:hidden;height:100%;-ms-content-zooming:none;-ms-touch-action:none;word-break:break-all;-ms-word-break:break-all}
body,input,textarea,select,button,table,pre{font-family:'돋움',Dotum,Helvetica,'Apple SD Gothic Neo',Sans-serif;font-size:12px}
body{scrollbar-3dlight-color:#f4f4f4;scrollbar-arrow-color:#8c8c8c;scrollbar-track-color:#f4f4f4;scrollbar-darkshadow-color:#f4f4f4;scrollbar-face-color:#fff;scrollbar-highlight-color:#c3c3c3;scrollbar-shadow-color:#c3c3c3}
img,fieldset{border:0}
menu,ul,ol{list-style:none}
em,address{font-style:normal}
a,a:link{text-decoration:none}
a:hover,a:active,a:focus{text-decoration:underline}
textarea{overflow:auto}
option,x:-moz-any-link{padding-right:6px}
option,x:-moz-any-link,x:default{padding-right:0}
input[type=text]::-ms-clear{display:none}
input[type='file']::-webkit-file-upload-button{cursor:pointer}
input[type='checkbox'],input[type='radio']{width:13px;height:13px;margin:-1px \0/IE9;padding:1px \0/IE9}
legend,.blind{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:0;height:0;font-size:0;line-height:0}
button,label{cursor:pointer}
button::-moz-focus-inner{padding:0}
hr{display:none}
caption{overflow:hidden;position:relative;top:-100px;left:-100px;width:0;height:0;font-size:0;line-height:0}

/* for Map */


/* Design Scroll */
.nano{overflow:hidden;width:100%}
.nano .content{overflow:hidden;overflow-y:scroll;position:absolute;top:0;right:0;bottom:0;left:0}
.nano .content::-webkit-scrollbar{visibility:hidden}
.has-scrollbar .content::-webkit-scrollbar{visibility:visible}
.nano .pane{overflow:hidden;position:absolute;top:0;right:0;bottom:0;z-index:100;visibility:hidden\9;width:8px;opacity:0;transition:.2s;-o-transition:.2s;-moz-transition:.2s;-webkit-transition:.2s}
.nano .pane .slider{position:relative;z-index:101;width:8px}
.nano .pane span{background:url(http://static.naver.net/maps/ux2013/bg_scroll.png) no-repeat}
.nano .pane .pane_t{position:absolute;top:0;left:0;width:8px;height:10px;background-position:-13px 0}
.nano .pane .pane_m{position:absolute;top:10px;right:0;bottom:10px;left:0;z-index:100;width:8px;background-position:-26px 0;background-repeat:repeat-y}
.nano .pane .pane_b{position:absolute;bottom:0;left:0;width:8px;height:10px;background-position:0 0}
.nano .pane .pane_slider_t{position:absolute;top:0;left:0;width:8px;height:10px;background-position:-52px 0}
.nano .pane .pane_slider_m{position:absolute;top:10px;right:0;bottom:10px;left:0;z-index:100;width:8px;background-position:-65px 0;background-repeat:repeat-y}
.nano .pane .pane_slider_b{position:absolute;bottom:0;left:0;width:8px;height:10px;background-position:-39px 0}
.nano:hover .pane,.pane.active,.pane.flashed{visibility:visible\9;opacity:1}

/* Skip Navigation */
#u_skip{position:relative;z-index:2147483647}
#u_skip a{position:absolute;left:0;top:-30px;width:108px;background:url(http://static.naver.net/maps/ux2013/sp_map_vertical_v4.png) repeat-x 0 -91px;border:1px solid #4ec53d;text-align:center;outline:none}
#u_skip a:active,#u_skip a:focus{top:0;text-decoration:none;cursor:pointer;zoom:1}
#u_skip span{display:inline-block;padding:2px 6px 0 0;background:url(http://static.naver.net/maps/ux2013/sp_map_vertical_v4.png) no-repeat 100% -69px;font-size:11px;letter-spacing:-1px;color:#fff;line-height:26px}



/* Footer Company */
.footer_company{position:absolute;bottom:2px;right:165px;height:0;font-family:'나눔고딕',NanumGothic,'돋움',Dotum,Helvetica,'Apple SD Gothic Neo',Sans-serif;font-size:11px;white-space:nowrap;line-height:18px}
.nmap_mover_hide .footer_company{right:29px}
.footer_company address{margin:-19px 64px 0 0}
.footer_company address .nhn{display:inline-block}
.footer_company .spmh_naver{margin:6px 0 0 10px}
.footer_company .scale_len{position:absolute;top:-12px;right:8px}
.footer_company .scale_len em{position:absolute;top:-12px;left:0;width:50px;font-family:'tahoma',Helvetica,'Apple SD Gothic Neo',Sans-serif;font-size:9px;text-align:center}
.wrap_satellite .footer_company .scale_len em{color:#fff}

/* Aside Footer */
.aside_footer{text-align:center}
.afooter_lst{display:inline-block;*display:inline;zoom:1;padding:0;text-align:left;line-height:26px}
.afooter_lst > li{float:left;margin-left:0;padding-left:0;vertical-align:top}


/* Copyright */
.copyright{position:static;height:0;font-family:'나눔고딕',NanumGothic,'돋움',Dotum,Helvetica,'Apple SD Gothic Neo',Sans-serif;font-size:11px;white-space:nowrap;line-height:17px}
.copyright a.limit{margin-left:10px;padding-right:6px;background:url(http://static.naver.net/maps/ux2013/sp_map_vertical_v4.png) no-repeat 100% -216px;color:#000;font-weight:bold}
.copyright p{position:absolute;bottom:4px;left:10px}
.wrap_satellite .copyright{color:#fff}
.wrap_satellite .copyright a.limit{color:#fff}


/* ---------- LAYOUT ---------- */

/* Box */
